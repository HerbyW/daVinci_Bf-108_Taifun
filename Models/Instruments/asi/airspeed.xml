<?xml version="1.0"?>

<!-- Airspeed Indicator, by Dave Culp -->

<PropertyList>

 <path>AS.ac</path>
 <animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>velocities/airspeed-kt</property>
  <interpolation>
    <entry>
      <ind>0</ind>
      <dep>0.0</dep>
    </entry>
    <entry>
      <ind>50</ind>
      <dep>20.0</dep>
    </entry>
    <entry>
      <ind>100</ind>
      <dep>85.0</dep>
    </entry>
   <entry>
      <ind>120</ind>
      <dep>110.0</dep>
    </entry>
   <entry>
      <ind>150</ind>
      <dep>135.0</dep>
    </entry>
    <entry>
      <ind>200</ind>
      <dep>185.0</dep>
    </entry>
   <entry>
      <ind>250</ind>
      <dep>230.0</dep>
    </entry>
    <entry>
      <ind>300</ind>
      <dep>275.0</dep>
    </entry>
   <entry>
      <ind>350</ind>
      <dep>310.0</dep>
    </entry>
    <entry>
      <ind>400</ind>
      <dep>350.0</dep>
    </entry>
  </interpolation>   
  <center>
   <x-m>-0.0196</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>material</type>
  <object-name>face</object-name>
  <object-name>needle</object-name>
   <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
 </animation>    

 
 <!-- *** LoD *** -->
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5</max-m>
 </animation>

</PropertyList>
